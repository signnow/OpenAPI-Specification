  /oauth2/token:
    post:
      responses:
        '200':
          description: ''
          schema:
            type: object
            properties:
              expires_in:
                type: integer
              token_type:
                type: string
              access_token:
                type: string
              refresh_token:
                type: string
              scope:
                type: string
              last_login:
                type: integer
          examples:
            application/json:
              expires_in: 2592000
              token_type: bearer
              access_token: 7cfe4b0fecbfaa0b4781c30262f0bf3f68b9b6ca5d11270fd83a091e968042d9
              refresh_token: 59889bcbced82d5c2ba1bc7d0f634f30521e3d47b7f96221a2d8618a30175a16
              scope: '*'
              last_login: 1
        '400':
          description: |-
            * "invalid_client" - incorrect basic auth token
            * "invalid_request" - request without a required parameter
            * "invalid_scope" - invalid scope
            * "Invalid credentials." - either incorrect password or non-existing email provided
          examples:
            invalid_client:
              error: invalid_client
            invalid_request:
              error: invalid_request
            invalid_scope:
              error: invalid_scope
            invalid_credentials:
              error: Invalid credentials.
              code: 826
      description: Returns an access token for a user
      tags:
        - OAuth2
      produces:
        - application/json
      consumes:
        - multipart/form-data
      parameters:
        - $ref: '#/parameters/auth-basic'
        - in: formData
          name: username
          type: string
          required: true
          description: |-
            user's email; <br>
            required if `'grant_type: password'`
        - in: formData
          name: password
          type: string
          required: true
          description: |-
            user's password;<br>
            required if `'grant_type: password'`
        - in: formData
          name: grant_type
          type: string
          default: password
          required: true
          description: 'can be `''password''`, `''refresh_token''`, `''authorization_code''`'
        - in: formData
          name: scope
          type: string
          default: '*'
        - in: formData
          name: refresh_token
          type: string
          description: 'required if `''grant_type: refresh_token''`'
          default: '{{refresh_token}}'
        - in: formData
          name: code
          type: string
          default: '{{authorization_code}}'
          description: 'required if `''grant_type: authorization_code''`'
        - in: formData
          name: expiration_time
          type: string
          description: the amount of time till the token expires in seconds.
      summary: Generate access token
    get:
      responses:
        '200':
          description: 'Returns the access_token itself as well as its'' scope, expiration date, and type.'
          schema:
            type: object
            properties:
              access_token:
                type: string
              scope:
                type: string
              expires_in:
                type: string
              token_type:
                type: string
          examples:
            application/json:
              access_token: 8d83aed6d902c396e2a45d73eb0f5e41cfa11caa726e621247dc15c46d5fb112
              scope: '*'
              expires_in: '2517898'
              token_type: bearer
        '400':
          description: '"invalid_token" - either incorrect or expired token provided'
          examples:
            invalid_token:
              error: invalid_token
      summary: Verify access token
      description: Verify an access token for a user
      tags:
        - OAuth2
      produces:
        - application/json
      parameters:
        - $ref: '#/parameters/auth-bearer'
